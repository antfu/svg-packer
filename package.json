{
  "name": "svg-packer",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.11.0",
  "description": "Pack SVGs to Icon Fonts - In Browser!",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/svg-packer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/svg-packer"
  },
  "bugs": "https://github.com/antfu/svg-packer/issues",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "unpkg": "dist/index.browser.js",
  "jsdelivr": "dist/index.browser.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "prepack": "tsup",
    "postinstall": "node scripts/pack-wasm.js",
    "dev:serve": "nr prepack && pnpx serve .",
    "release": "bumpp && npm publish",
    "test:node": "vitest --project=node",
    "test:browser": "vitest --project=browser",
    "test:browser:headless": "CI=true vitest --project=browser",
    "test": "tsc --noEmit && vitest",
    "test:ci": "tsc --noEmit && vitest run"
  },
  "dependencies": {
    "client-zip": "2.5.0",
    "readable-stream": "latest",
    "svg2ttf": "6.0.3",
    "svgicons2svgfont": "15.0.1",
    "ttf2eot": "3.1.0",
    "ttf2woff": "3.0.0",
    "ttf2woff2": "6.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.1",
    "@antfu/ni": "^24.4.0",
    "@vitest/browser": "^3.1.4",
    "bumpp": "^10.1.1",
    "esbuild": "^0.25.4",
    "eslint": "^9.27.0",
    "node-stdlib-browser": "^1.3.1",
    "playwright": "^1.52.0",
    "std-env": "^3.9.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "^3.1.4"
  }
}
